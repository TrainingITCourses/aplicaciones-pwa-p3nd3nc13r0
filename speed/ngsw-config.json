{
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/*.css",
                "/*.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/icons/**",
                "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
            ]
        }
    }],
    "dataGroups": [{
            "name": "cache-first",
            "urls": [
                "http://127.0.0.1:8080/assets/data/agencies.json",
                "http://127.0.0.1:8080/assets/data/launchstatus.json",
                "http://127.0.0.1:8080/assets/data/missiontypes.json"
            ],
            "cacheConfig": {
                "strategy": "performance",
                "maxSize": 10,
                "maxAge": "1d"
            }
        },
        {
            "name": "api-first",
            "urls": [
                "http://127.0.0.1:8080/assets/data/launches.json"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 100,
                "maxAge": "1h",
                "timeout": "5s"
            }
        }
    ]
}